{
    "name": "caching-decorator",
    "version": "1.0.0",
    "description": "An in-memory caching (memoization) decorator for Typescript",
    "keywords": [
        "cache",
        "caching",
        "cacheable",
        "memoize",
        "memoization",
        "typescript",
        "decorator",
        "memoise",
        "memoisation",
        "performance",
        "scopes",
        "ttl"
    ],
    "files": [
        "dist/**/*"
    ],
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "author": "MSTS",
    "contributors": [{
        "name": "Schandro"
    }],
    "license": "MPL-2.0",
    "scripts": {
        "build": "rm -fr dist && tsc -p tsconfig.build.json --strict && npm run lint && npm test",
        "format": "prettier --write \"src/**/*.ts\"",
        "lint": "tslint -p tsconfig.json -c tslint.json",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage --forceExit",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register/transpile-only node_modules/.bin/jest --runInBand"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Schandro/caching-decorator"
    },
    "bugs": {
        "url": "https://github.com/Schandro/caching-decorator/issues"
    },
    "homepage": "https://github.com/Schandro/caching-decorator",
    "dependencies": {
    },
    "devDependencies": {
        "@types/jest": "^24.0.15",
        "@types/supertest": "^2.0.8",
        "express": "^4.17.1",
        "faker": "^4.1.0",
        "jest": "^24.8.0",
        "nodemon": "^1.19.1",
        "prettier": "^1.17.1",
        "statman-stopwatch": "^2.8.2",
        "supertest": "^4.0.2",
        "ts-jest": "^24.0.2",
        "ts-node": "^8.3.0",
        "tsconfig-paths": "^3.8.0",
        "tslint": "^5.18.0",
        "typescript": "^3.5.2"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "./",
        "testRegex": ".spec.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverage": false,
        "coverageReporters": [
            "json",
            "html"
        ],
        "coverageDirectory": "./coverage",
        "testEnvironment": "node"
    }
}
